issn = {1557-9964},
journal = {IEEE/ACM transactions on computational biology and bioinformatics / IEEE, ACM},
keywords = {Computational Biology,Computational Biology: methods,Databases, Genetic,Gene Regulatory Networks,Gene Regulatory Networks: physiology,Humans,Models, Biological,Protein Interaction Maps,RNA Interference,Signal Transduction,Signal Transduction: physiology},
number = {6},
pages = {1696--708},
