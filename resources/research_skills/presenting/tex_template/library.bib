month = {may},
number = {10 Suppl},
pages = {2199--203},
pmid = {8097962},
title = {{Heterogeneity of erbB-2 gene amplification in bladder cancer.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8097962},
volume = {53},
year = {1993}
}
