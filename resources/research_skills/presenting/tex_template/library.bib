@article{He,
abstract = {We design and implement Mars, a MapReduce framework, on graphics processors (GPUs). MapReduce is a distributed programming framework originally proposed by Google for the ease of development of web search applications on a large number of CPUs. Compared with commodity CPUs, GPUs have an order of magnitude higher computation power and memory bandwidth, but are harder to program since their architectures are designed as a special-purpose co-processor and their programming interfaces are typically for graphics applications. As the first attempt to harness GPU's power for MapReduce, we developed Mars on an NVIDIA G80 GPU, which contains hundreds of processors, and evaluated it in comparison with Phoenix, the state-of-the-art MapReduce framework on multi-core processors. Mars hides the programming complexity of the GPU behind the simple and familiar MapReduce interface. It is up to 16 times faster than its CPU-based counterpart for six common web applications on a quad-core machine. Additionally, we integrated Mars with Phoenix to perform co-processing between the GPU and the CPU for further performance improvement.},
author = {He, Bingsheng and Fang, Wenbin and Govindaraju, Naga K and Luo, Qiong and Wang, Tuyong},
file = {:Users/windels/Library/Application Support/Mendeley Desktop/Downloaded/He et al. - Unknown - Mars A MapReduce Framework on Graphics Processors.pdf:pdf},
title = {{Mars: A MapReduce Framework on Graphics Processors}}
}
@article{Knapp2013,
abstract = {Perturbation experiments for example using RNA interference (RNAi) offer an attractive way to elucidate gene function in a high throughput fashion. The placement of hit genes in their functional context and the inference of underlying networks from such data, however, are challenging tasks. One of the problems in network inference is the exponential number of possible network topologies for a given number of genes. Here, we introduce a novel mathematical approach to address this question. We formulate network inference as a linear optimization problem, which can be solved efficiently even for large-scale systems. We use simulated data to evaluate our approach, and show improved performance in particular on larger networks over state-of-the art methods. We achieve increased sensitivity and specificity, as well as a significant reduction in computing time. Furthermore, we show superior performance on noisy data. We then apply our approach to study the intracellular signaling of human primary nave CD4(+) T-cells, as well as ErbB signaling in trastuzumab resistant breast cancer cells. In both cases, our approach recovers known interactions and points to additional relevant processes. In ErbB signaling, our results predict an important role of negative and positive feedback in controlling the cell cycle progression.},
author = {Knapp, Bettina and Kaderali, Lars},
doi = {10.1371/journal.pone.0069220},
file = {:Users/windels/Library/Application Support/Mendeley Desktop/Downloaded/Knapp, Kaderali - 2013 - Reconstruction of cellular signal transduction networks using perturbation assays and linear programming.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Algorithms,CD4-Positive T-Lymphocytes,CD4-Positive T-Lymphocytes: metabolism,Cluster Analysis,Computational Biology,Computational Biology: methods,Computer Simulation,Humans,Models, Biological,Programming Languages,Programming, Linear,Protein Interaction Maps,Receptor, Epidermal Growth Factor,Receptor, Epidermal Growth Factor: metabolism,Signal Transduction},
month = {jan},
number = {7},
pages = {e69220},
pmid = {23935958},
publisher = {Public Library of Science},
title = {{Reconstruction of cellular signal transduction networks using perturbation assays and linear programming.}},
url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0069220#references},
volume = {8},
year = {2013}
}
