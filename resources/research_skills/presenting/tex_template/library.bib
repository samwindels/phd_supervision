pmid = {26072491},
title = {{Integrating different data types by regularized unsupervised multiple kernel learning with application to cancer subtype discovery}},
url = {https://watermark.silverchair.com/btv244.pdf?token=AQECAHi208BE49Ooan9kkhW_Ercy7Dm3ZL_9Cf3qfKAc485ysgAAAeQwggHgBgkqhkiG9w0BBwagggHRMIIBzQIBADCCAcYGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQMBg11v8g0k8sTB4vtAgEQgIIBl3ZtarS-k59qcn3B5X0M4KQAY62IvrXcNsXzW9UnQtM2INoQ},
