journal = {IEEE/ACM transactions on computational biology and bioinformatics / IEEE, ACM},
keywords = {Animals,Computational Biology,Computational Biology: methods,Databases, Protein,Drosophila melanogaster,Humans,Protein Interaction Maps,Protein Interaction Maps: physiology,RNA Interference,RNA Interference: physiology,Reproducibility of Results,Signal Transduction,Signal Transduction: physiology},
