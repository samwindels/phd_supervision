@article{Sachs2009,
abstract = {Flow cytometric measurement of signaling protein abundances has proved particularly useful for elucidation of signaling pathway structure. The single cell nature of the data ensures a very large dataset size, providing a statistically robust dataset for structure learning. Moreover, the approach is easily scaled to many conditions in high throughput. However, the technology suffers from a dimensionality constraint: at the cutting edge, only about 12 protein species can be measured per cell, far from sufficient for most signaling pathways. Because the structure learning algorithm (in practice) requires that all variables be measured together simultaneously, this restricts structure learning to the number of variables that constitute the flow cytometer's upper dimensionality limit. To address this problem, we present here an algorithm that enables structure learning for sparsely distributed data, allowing structure learning beyond the measurement technology's upper dimensionality limit for simultaneously measurable variables. The algorithm assesses pairwise (or n-wise) dependencies, constructs "Markov neighborhoods" for each variable based on these dependencies, measures each variable in the context of its neighborhood, and performs structure learning using a constrained search.},
author = {Sachs, Karen and Itani, Solomon and Carlisle, Jennifer and Nolan, Garry P and Pe'er, Dana and Lauffenburger, Douglas A},
doi = {10.1089/cmb.2008.07TT},
file = {:Users/windels/Library/Application Support/Mendeley Desktop/Downloaded/Sachs et al. - 2009 - Learning signaling network structures with sparsely distributed data.pdf:pdf},
issn = {1557-8666},
journal = {Journal of computational biology : a journal of computational molecular cell biology},
keywords = {Algorithms,Artificial Intelligence,Bayes Theorem,Flow Cytometry,Markov Chains,Models, Biological,Signal Transduction},
month = {feb},
number = {2},
pages = {201--12},
pmid = {19193145},
title = {{Learning signaling network structures with sparsely distributed data.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3198894&tool=pmcentrez&rendertype=abstract},
volume = {16},
