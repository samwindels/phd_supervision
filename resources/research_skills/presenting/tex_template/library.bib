keywords = {Algorithms,Automatic Data Processing,Biological,Breast Neoplasms,Breast Neoplasms: metabolism,Cell Line, Tumor,Computer Simulation,Female,Humans,Likelihood Functions,Models, Biological,Neoplasm Proteins,Neoplasm Proteins: metabolism,Phosphorylation,Protein Array Analysis,Proteins,Proteins: metabolism,Proteomics,Proteomics: methods,Signal Transduction,Systems Biology,Systems Biology: methods,Tumor},
month = {sep},
