issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Causality,Computer Simulation,Gene Expression Regulation,Gene Expression Regulation: physiology,Gene Silencing,Gene Silencing: physiology,Models, Biological,Proteome,Proteome: metabolism,Research Design,Signal Transduction,Signal Transduction: physiology,Software},
month = {jul},
number = {13},
pages = {i359--66},
pmid = {17646318},
title = {{SPINE: a framework for signaling-regulatory pathway inference from cause-effect experiments.}},
url = {http://bioinformatics.oxfordjournals.org/content/23/13/i359.full},
volume = {23},
year = {2007}
}
